---
layout: none
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <author>
    <name>The OSCAR Team</name>
  </author>
  <title>OSCAR blog</title>
  <id>http://oscar.computeralgebra.de</id>
  <updated>{{ site.time }}</updated>

{% for post in site.categories.blogs %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="{{ site.url}}{{ site.baseurl }}{{ post.url | absolute }}"/>
    <id>{{ site.url }}{{ site.baseurl }}{{ post.url | absolute }}</id>
    <updated>{{ post.date }}</updated>
  </entry>
{% endfor %}
</feed>